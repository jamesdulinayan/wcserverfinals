n\na:hover{\n    opacity: 5;\n    color: rgb(155,127,58);\n    transition: all .5s;\n}\n\n.profile {\n  position: absolute;\n  right: 5rem;\n  top: 2rem;\n  border: 1.5px solid #fff;\n  border-radius: 0.75rem;\n  padding: 0.75rem 1.25rem;\n  transition: all 0.3s ease-in-out;\n}\n\n.profile:hover {\n  color: white;\n}\n\n.wrapper {\n  height: 100vh;\n  overflow-x: hidden;\n  overflow-y: auto;\n  perspective: 300px;\n}\n\n.parallax__group {\n  position: relative;\n  height: 100vh;\n  width: 100vw;\n  transform-style: preserve-3d;\n}\n\n.parallax__layer {\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n}\n\n\n.water {\n  background: url('/images/horizon2.png') no-repeat center;\n  background-size: cover;\n  transform: translateZ(-400px) scale(2.33333333);\n  z-index: 3;\n}\n\n.people2 {\n  background: url('/images/horizon.png') no-repeat center;\n  background-size: cover;\n  transform: translateZ(-125px) scale(1.4167);\n  z-index: 5;\n}\n\n.people3 {\n  background: url('/images/horizon3.png') no-repeat center;\n  background-size: cover;\n  transform: translateZ(0) scale(1);\n  z-index: 6;\n}\n\n\n.hero-text {\n  background-size: cover;\n  transform: translateZ(0) scale(1);\n  z-index: 7;\n}\n\n.hero-text img {\n  width: 150px;\n  height: 150px;\n  font-size: 1.25rem;\n  position: absolute;\n  left: 4rem;\n  top: -0.5rem;\n}\n\n.hero-text ul {\n  font-size: 0.75rem;\n  position: absolute;\n  right: 15rem;\n  top: 3rem;\n}\n\n.hero-text ul li {\n  margin-left: 3rem;\n  list-style-type: none;\n  float: left;\n  visibility: hidden;\n}\n\n.hero-text ul li:nth-of-type(3) {\n  visibility: visible;\n}\n\n.info-container {\n  position: relative;\n  height: 50%;\n  background: black;\n  transform: translateZ(0) scale(1);\n  z-index: 8;\n  display: flex;\n  flex-direction: column;\n}\n\n.info-container p{\n  line-height: 1.5em;\n  font-size: 20px;\n  text-align: center;\n}\n\n.info-container h1{\n  line-height: 1;\n  font-size: 70px;\n  text-align: center;\n  letter-spacing: 1.5px;\n  font-family: 'Monospace';\n  font-weight: normal;\n  padding-top: 10%;\n}\n\n\n.text-container p:nth-of-type(1) {\n  font-weight: 400;\n  font-size: 0.75rem;\n  margin: 1rem 2.5rem;\n}\n\n.text-container p:nth-of-type(2) {\n  font-size: 0.6rem;\n  margin-bottom: 3rem;\n}\n\n@media (min-width: 760px) {\n  .hero-text ul li {\n    visibility: visible;\n  }\n\n  .info-container {\n    align-items: center;\n    text-align: left;\n  }\n\n  .text-container p:nth-of-type(1) {\n    font-size: 1rem;\n    width: 75%;\n    margin: 1rem 0;\n  }\n\n  .text-container p:nth-of-type(1) {\n    font-size: 0.86rem;\n  }\n}\n\n/* ------------ */\n\n.section{\n  position: relative;\n  padding-top: 100px;\n  background-size: cover;\n  background-position: center;\n  width: 100%;\n}\n\n/*-----------------------------*/\n\n\n.section-astronomy{\n  float: left;\n  margin: 0;\n  background-image:url('/images/titanfooter.jpg');\n  height: 4991px\n\n}\n\n/*--------------------------*/\n\n.section-mars{\n  float: left;\n  background: black;\n  height: 700px;\n  color:white;\n}\n\n\n.section-mars p{\n  font-family: 'Futura';\n  line-height: 23px;\n  margin-left: 25%;\n  margin-right: 25%;\n  padding-top: 25px;\n  font-size: 15px;\n}\n\n\n.button {\n  text-transform: uppercase;\n  background-color: transparent;\n  border: 3px solid white;\n  color: white;\n  padding: 16px 32px;\n  text-align: center;\n  letter-spacing: 3px;\n  font-size: 12px;\n  margin-top: 4%;\n  display: inline-block;\n  text-decoration: none;\n  font-family: 'Tw Cen MT';\n}\n\n.button:hover {\n  opacity: 5;\n  color: black;\n  transition: 0.3s;\n  background-color: white;\n\n}\n\nspan{\n  padding-top: 2%;\n  text-align: center;\n}\n\nfooter{\n  padding-top: 50px;\n  letter-spacing: 2px;\n  height: 20%;\n  width: 100%;\n  background-color: rgb(24, 24, 24); \n}\n\nfooter span{\n  color: crimson\n  \n}\n\nfooter img{\n  width: 100px;\n}\n\n</style>\n\n"]}]}